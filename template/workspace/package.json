{
  "name": "kotori-workpsace",
  "description": "kotori workpsace project",
  "version": "1.0.0",
  "private": true,
  "license": "GPL-3.0",
  "author": "AUTHOR",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm build && pnpm exec kotori",
    "lint": "eslint \"project/*/src/*.{ts,tsx}\" --fix",
    "format": "prettier --config .prettierrc \"project/*/src/*.ts\" --write"
  },
  "files": [
    "project/*/locales/**/*",
    "project/*/{lib,dist}/**/*.js",
    "project/*/{package.json,LICENSE,README.md}",
    "README.md",
    "LICENSE",
    "pnpm-workspace.yaml",
    "kotori.yml",
    "kotori.db"
  ],
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-typescript": "^3.0.0",
    "prettier": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "kotori-bot": "^1.3.1"
  },
  "packageManager": "pnpm@8.7.4",
  "engines": {
    "node": ">=17.9.0"
  }
}
